{
  "luci-app-v2ray": {
    "description": "Grant access to LuCI app V2ray",
    "read": {
      "cgi-io": [ "exec" ],
      "uci": [ "firewall", "v2ray" ],
      "file": {
        "/etc/v2ray/*": ["read"]
      }
		},
		"write": {
      "uci": [ "firewall", "v2ray" ],
      "file": {
        "/etc/v2ray/*": ["write"]
      }
		}
  }
}
