config v2ray 'main'
  option enabled '0'
  option v2ray_file '/usr/bin/v2ray'
  option v2ctl_file '/usr/bin/v2ctl'
  option loglevel 'none'
  option access_log ''
  option error_log ''
  option dns_enabled '0'
  option stats_enabled '0'
  option routing_enabled '0'
  option policy_enabled '0'
  option reverse_enabled '0'
  option transport_enabled '0'

config dns 'main_dns'
  option tag 'dns_inbound'

config routing 'main_routing'
  option domain_strategy 'AsIs'

config policy 'main_policy'
  option system_stats_inbound_uplink 'false'
  option system_stats_inbound_downlink 'false'

config reverse 'main_reverse'
