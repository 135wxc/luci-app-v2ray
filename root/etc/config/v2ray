config v2ray 'main'
	option enabled '0'
	option v2ray_file '/usr/bin/v2ray'
	option config_file ''
	option loglevel 'warning'
	option access_log '/var/run/v2ray-access.log'
	option error_log '/var/run/v2ray-access.log'
	option stats_enabled '0'
	option transport_enabled '0'

config dns 'main_dns'
	option enabled '0'
	option tag 'dns_inbound'

config dns_server 'google_dns_1'
	option alias 'google_dns_1'
	option address '8.8.8.8'

config dns_server 'ali_dns_1'
	option alias 'ali_dns_1'
	option address '223.5.5.5'
	option port '53'
	list domains 'domain:v2ray.com'

config dns_server '114_dns_1'
	option alias '114_dns_1'
	option address '114.114.114.114'

config routing 'main_routing'
	option enabled '0'
	option domain_strategy 'IPOnDemand'

config routing_rule 'routing_rule_1'
	option alias 'routing_rule_1'
	option type 'field'
	list ip 'geoip:private'
	option outbound_tag 'direct'

config policy 'main_policy'
	option enabled '0'

config policy_level 'policy_level_1'
	option level '0'
	option handshake '4'
	option conn_idle '300'
	option uplink_only '2'
	option downlink_only '5'
	option buffer_size '0'

config reverse 'main_reverse'
	option enabled '0'
	list bridges 'bridge|test.v2ray.com'
	list portals 'portal|test.v2ray.com'

config inbound 'inbound_1'
	option alias 'inbound_1'
	option port '1080'
	option listen '127.0.0.1'
	option protocol 'socks'

config outbound 'outbound_1'
	option alias 'outbound_1'
	option protocol 'vmess'

config outbound 'outbound_2'
	option alias 'outbound_2'
	option protocol 'freedom'
	option tag 'direct'
